{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://voxeronai-tech.github.io/voxeron-architecture/schemas/domain/dispatch_result.v0.6.json",
  "title": "DispatchResult v0.6",
  "type": "object",
  "additionalProperties": false,
  "required": ["status", "attempts"],
  "properties": {
    "status": { "type": "string", "enum": ["dispatched", "queued", "failed"] },
    "attempts": { "type": "integer", "minimum": 0, "maximum": 10 },
    "edge_agent_id": { "type": ["string", "null"] },
    "error": { "type": ["string", "null"] }
  }
}
